<%= form_for @rent do  |f| %>    
    <%= f.text_field :name%>
    <%= f.radio_button :facility, '4' %> 
    <%= f.label :facility_4, 'I202' %>
    <%= f.radio_button :facility, '5'%>
    <%= f.label :facility_5, 'I210' %>
    <%= f.radio_button :facility, '6'%>
    <%= f.label :facility_6, 'I002' %>
    <%= f.datetime_select :start%>
    <%= f.datetime_select :end%>
    <%= f.submit %>
<% end %>
